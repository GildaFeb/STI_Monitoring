                            <div class="col-md-10">
                                @Html.DropDownListFor(model => model.Type, new List<SelectListItem>() { new SelectListItem() { Text = "Guest", Value = "Guest" }, new SelectListItem() { Text = "Guard", Value = "Guard" }, new SelectListItem() { Text = "Cashier", Value = "Cashier" }, new SelectListItem() { Text = "Front Desk", Value = "FrontDesk" } }, new { @class = "form-control" })

