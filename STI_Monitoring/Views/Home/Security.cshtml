<head>



    <!-- Favicons -->
    <!-- Main Stylesheet File -->
    <link href="~/Content/CSS/style.css" rel="stylesheet">
    <style>
        .containers {
            position: relative;
            width: 100%;
        }

        .images {
            display: block;
            width: 100%;
            height: auto;
        }

        .overlays {
            position: absolute;
            bottom: 0;
            left: 0;
            right: 0;
            background-color: white;
            overflow: hidden;
            width: 100%;
            height: 0;
            transition: .5s ease;
        }

        .containers:hover .overlays {
            height: 100%;
        }

        .texts {
            color: white;
            font-size: 25px;
            font-weight: bold;
            position: absolute;
            top: 50%;
            left: 50%;
            -webkit-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
            text-align: center;
        }
    </style>
</head>
<body>
    <!--==========================
      Services Section
    ============================-->
    <section id="services" style="background-color:#003366">
        <div class="container">

            <header class="section-header" style="color:white">
                <h3 style="color:white"> Guest Request Information</h3>
                <p>---------------------------------------------------</p>
            </header>

            <div style="margin-right: 10%;">

                <div class="col-md-12 col-lg-5 offset-lg-1 wow bounceInUp" data-wow-duration="1.4s">
                    <div class="intro-img">
                        <div class="containers">
                            <img src="~/Content/img/guest.gif" s alt="avatars" class="images">
                            <div class="overlays">
                                <h4 class="title texts">@Html.ActionLink("Guest Request", "RequestMonitoring", "Home", new { htmlAttributes = new { color = "white", style = "bold" } })</h4>
                            </div>
                        </div>
                        <div class="col-md-12 col-lg-12 offset-lg-1 wow bounceInUp" data-wow-duration="1.4s" style="margin-top: 10%">
                            <div class="box">
                                <h4>@Html.ActionLink("Guest Request", "RequestMonitoring", "Home", new { htmlAttributes = new { color = "#fff200", style = "bold" } })</h4>
                                <p style="font-size:large; color:white;">Guard is assigned to monitor and update time and date when guest enter and exit STI College San Jose Del Monte</p>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-lg-5 offset-lg-1 wow bounceInUp" data-wow-duration="1.4s">
                    <div class="intro-img">
                        <div class="containers">
                            <img src="~/Content/img/peoples.gif" s alt="avatars" class="images">
                            <div class="overlays">
                                <h4 class="title texts">@Html.ActionLink("Guest Transactions", "GuestTransactions", "Home", new { htmlAttributes = new { color = "white" }, style = "bold" })</h4>
                            </div>
                        </div>
                        <div class="col-md-12 col-lg-12 offset-lg-1 wow bounceInUp" data-wow-duration="1.4s" style="margin-top: 10%">
                            <div class="box">
                                <h4>@Html.ActionLink("Guest Transactions", "GuestTransactions", "Home", new { htmlAttributes = new { color = "#fff200" }, style = "bold" })</h4>
                                <p style="font-size:large; color:white;"> This section contains an information about the transaction of guest inside STI College San Jose Del Monte</p>
                            </div>
                        </div>
                    </div>
                </div>




            </div>

        </div>
    </section><!-- #services -->

</body>

